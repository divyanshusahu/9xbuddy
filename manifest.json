{
  "name": "9xbuddy",
  "version": "0.0.1",
  "description": "An easy one-click way to download videos using 9xbuddy.in",
  "manifest_version": 2,
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
